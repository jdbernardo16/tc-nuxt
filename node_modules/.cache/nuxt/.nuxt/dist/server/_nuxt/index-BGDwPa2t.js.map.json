itle: siteName,\n    description: description,\n    ogDescription: shortDescription,\n    ogImage: siteImage,\n    twitterCard: `summary_large_image`,\n})\n</script>\n\n<template>\n    <main>\n        <HeroBanner />\n\n        <section class=\"container my-16\">\n            <div class=\"flex items-end justify-between\">\n                <h2 class=\"text-lg font-semibold md:text-2xl\">{{ $t('messages.shop.shopByCategory') }}</h2>\n                <NuxtLink\n                    class=\"text-primary\"\n                    to=\"/categories\"\n                    >{{ $t('messages.general.viewAll') }}</NuxtLink\n                >\n            </div>\n            <div class=\"grid justify-center grid-cols-2 gap-4 mt-8 md:grid-cols-3 lg:grid-cols-6\">\n                <CategoryCard\n                    v-for=\"(category, i) in productCategories\"\n                    :key=\"i\"\n                    class=\"w-full\"\n                    :node=\"category\"\n                />\n            </div>\n        </section>\n\n        <section class=\"container grid gap-4 my-24 md:grid-cols-2 lg:grid-cols-4\">\n            <div class=\"flex items-center gap-8 p-8 bg-white rounded-lg\">\n                <img\n                    src=\"/icons/box.svg\"\n                    width=\"60\"\n                    height=\"60\"\n                    alt=\"Free Shipping\"\n                    loading=\"lazy\"\n                />\n                <div>\n                    <h3 class=\"text-xl font-semibold\">Free Shipping</h3>\n                    <p class=\"text-sm\">Free shipping on order over â‚¬50</p>\n                </div>\n            </div>\n            <div class=\"flex items-center gap-8 p-8 bg-white rounded-lg\">\n                <img\n                    src=\"/icons/moneyback.svg\"\n                    width=\"60\"\n                    height=\"60\"\n                    alt=\"Money Back\"\n                    loading=\"lazy\"\n                />\n                <div>\n                    <h3 class=\"text-xl font-semibold\">Peace of Mind</h3>\n                    <p class=\"text-sm\">30 days money back guarantee</p>\n                </div>\n            </div>\n            <div class=\"flex items-center gap-8 p-8 bg-white rounded-lg\">\n                <img\n                    src=\"/icons/secure.svg\"\n                    width=\"60\"\n                    height=\"60\"\n                    alt=\"Secure Payment\"\n                    loading=\"lazy\"\n                />\n                <div>\n                    <h3 class=\"text-xl font-semibold\">100% Payment Secure</h3>\n                    <p class=\"text-sm\">Your payment are safe with us.</p>\n                </div>\n            </div>\n            <div class=\"flex items-center gap-8 p-8 bg-white rounded-lg\">\n                <img\n                    src=\"/icons/support.svg\"\n                    width=\"60\"\n                    height=\"60\"\n                    alt=\"Support 24/7\"\n                    loading=\"lazy\"\n                />\n                <div>\n                    <h3 class=\"text-xl font-semibold\">Support 24/7</h3>\n                    <p class=\"text-sm\">24/7 Online support</p>\n                </div>\n            </div>\n        </section>\n\n        <section\n            class=\"container my-16\"\n            v-if=\"popularProducts\"\n        >\n            <div class=\"flex items-end justify-between\">\n                <h2 class=\"text-lg font-semibold md:text-2xl\">{{ $t('messages.shop.popularProducts') }}</h2>\n                <NuxtLink\n                    class=\"text-primary\"\n                    to=\"/products\"\n                    >{{ $t('messages.general.viewAll') }}</NuxtLink\n                >\n            </div>\n            <ProductRow\n                :products=\"popularProducts\"\n                class=\"grid-cols-2 md:grid-cols-4 lg:grid-cols-5 mt-8\"\n            />\n        </section>\n    </main>\n</template>\n\n<style scoped>\n.brand im